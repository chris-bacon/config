URxvt*termName: rxvt-unicode

!URxvt.font: xft:monospace:size=12
!xterm*faceName: xft:DejaVu Sans Mono Bold for Powerline:pixelsize=14:antialias=true
!URxvt*font: xft:DejaVu Sans Mono:bold:pixelsize=14:antialias=true,xft:DejaVu Sans Mono for Powerline:bold:pixelsize=14:antialias=true
URxvt*font: xft:DejaVu Sans Mono for Powerline:pixelsize=14:antialias
URxvt.scrollBar: false

URxvt*background: rgba:0000/0000/0200/c800
URxvt*foreground: #d6d6d6
URxvt*cursorColor: #d6d6d6

URxvt*depth: 32

URxvt.iso14755: false
URxvt.iso14755_22: false
uxvt.keysym.Shift-Control-V: perl:clipboard:paste
uxvt.keysym.Shift-Control-C: perl:clipboard:copy

URxvt.perl-ext-common: default,clipboard
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Left: \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C
